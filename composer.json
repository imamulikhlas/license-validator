{
    "name": "alexafers/license-validator",
    "description": "System performance monitoring and optimization for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexa Fers",
            "email": "alexa@fers.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "alexafers\\SystemUtility\\": "src/"
        },
        "files": [
            "src/bootstrap.php",
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "alexafers\\SystemUtility\\Providers\\SystemServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}